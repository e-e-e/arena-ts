{
  "name": "arena-ts",
  "version": "1.0.0",
  "description": "A typescript API client for Are.na",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
