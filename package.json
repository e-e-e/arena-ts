    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.1.1",
    "ts-jest": "^26.4.0",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.2",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
